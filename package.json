{
	"name": "datum-merge",
	"version": "0.5.1",
	"author": "Rohit Kulkarni <rohk@live.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/therohk/datum-merge.git"
	},
	"license": "MIT",
	"description": "Simplified diff and merging for deeply nested objects.",
	"keywords": ["merge", "diff", "compare", "patch", "update"],
	"main": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": "^20",
		"npm": ">=10"
	},
	"scripts": {
		"lint": "npx eslint src",
		"pretty": "prettier -c src",
		"scantask": "findstr /s /i todo src\\*.*",
		"test": "jest",
		"build:esm": "tsc",
		"build:cjs": "tsc --module CommonJS --outDir dist/cjs",
		"build": "run-s test build:*",
		"compile": "tsc -noEmit -skipLibCheck",
		"cyccheck": "dpdm --exit-code circular:1 --no-tree ./src"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"@types/lodash": "^4.14.169",
		"@types/lodash-es": "^4.17.12",
		"@types/jest": "^29.5.2",
		"eslint": "^8.0.1",
		"prettier": "^3.1.0",
		"npm-run-all2": "^6.2.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "5.4.5",
		"lodash": "^4.17.21"
	},
	"dependencies": {
		"fast-deep-equal": "^3.1.3",
		"lodash-es": "^4.17.21"
	}
}